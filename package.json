{
  "name": "fido2-server-demo",
  "version": "1.0.0",
  "description": "Examples and demo code for FIDO2 / WebAuthn servers",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "start": "scm",
    "docker:build:prod-canary": "docker build --target=production -t yuki1967/fido2-server:prod-canary .",
    "docker:build:test-canary": "docker build --target=test -t yuuki1967/fido2-server:test-canary .",
    "docker:test:canary": "docker run yuuki1967/fido2-server:test-canary",
    "docker:build:release": "docker build --target=test -t yuki1967/fido2-server:test . && docker run yuki1967/fido2-server:test && docker build --target=production -t yuki1967/fido2-server:latest .",
    "docker:test:canary": "docker run yuki1967/fido2-server:test-canary",
    "docker:login": "echo \"$DOCKER_PASSWORD\" | docker login -u \"$DOCKER_USERNAME\" --password-stdin",
    "docker:release:canary": "docker push yuki1967/fido2-server:test-canary && docker push yuki1967/fido2-server:prod-canary",
    "docker:release": "docker push yuuki1967/fido2-server:test && docker push yuki1967/fido2-server:latest"
  },
  "keywords": [
    "fido",
    "fido2",
    "webauthn",
    "server"
  ],
  "author": "Adam Powers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuuki1967/fido2-server-demo.git"
  },
  "bugs": {
    "url": "https://github.com/yuuki1967/fido2-server-demo/issues",
    "email": "apowers@ato.ms"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "gh-pages": "^0.12.0",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "jsdoc": "^3.4.3",
    "mocha": "^2.5.3",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "component-certs-static": "^1.0.1",
    "component-fido2": "^1.0.11",
    "component-logger-winston": "^1.0.1",
    "component-uds-json": "^1.0.3",
    "component-web": "^1.0.1",
    "simple-component-manager": "^1.0.4"
  },
  "homepage": "https://github.com/yuuki1967/fido2-server-demo#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
